<!DOCTYPE html>
<html>
    <head>
        <%- include('meta/header') %> 
        <link href="css/design.css" rel="stylesheet">
    </head>
    <body>
        <%- include('meta/navbar') %> 
        <div class="container" id="content">
            <div class="row">
                
                <div class="col-12">
                    <h1> EVALUERINGER </h1>
                </div>
            </div>
                <div class="col-10">
                    <button id="design-evalueringer"> Tryk for design </button><br>
                    <img id="design-evalueringerPage">
                </div>

            <section>
                <div class="row">
            
                    <div class="col">
                        <div class="quizzes">
                            <h2> QUIZ </h2>
                        <% evalueringerContent.forEach(result =>{ %>
                            <a href="/evalueringer/<%= result.content.quiz %>"> 
                                <img id="quiz" src="img/Design/quiz.png">
                            </a><br>
                        <% }) %>
                        </div>
                    </div>

                    <div class="col">
                        <div class="flashcards">
                             <h2> FLASHCARDS </h2>
                        <% evalueringerContent.forEach(result =>{ %>
                            <a href="/evalueringer/<%= result.content.flashcard %>"> 
                                <img id="flashcard" src="img/Design/flashcard.png">
                            </a><br>
                        <% }) %>
                        </div>
                    </div>

                </div>




            </section>
        </div>
        <%- include('meta/footer') %> 
    </body>
    <script src="js/view-design.js"></script>
</html>