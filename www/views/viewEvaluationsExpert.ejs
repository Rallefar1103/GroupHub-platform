<!DOCTYPE html>
<html>
  <head>
    <%- include('meta/header') %> 
    <title>GroupHub | Evalueringer</title>
  </head>
  <body>
    <%- include('meta/navbar') %> 

    <h1>Evalueringer du har oprettet</h1>
    <% data.evaluations.forEach(evaluation => { %>
      <div class="container">
        <div class="row">
          <div class="col">
            <p class="evaluationTitle"> <strong>Titel:</strong> <%= evaluation.title %> </p>
          </div>
          <div class="col">
            <p class="evaluationSection"><strong>Sektion:</strong> <%= evaluation.idEvaluation %></p>
          </div>
          <div class="col">
            <p class="evaluationSection"><strong>Dokument:</strong> <%= evaluation.idDocument %></p>
          </div>
          <div class="col">
            <a class="btn btn-primary" href="<%=evaluation.idEvaluation%>">GÃ¥ til denne evaluering</a>
          </div>
        </div>
      </div>
    <% }); %>

    <%- include('meta/footer') %> 
  </body>
</html>